{
    "source_artifact_provider": {
        "name": "text",
        "args": {"artifact_type": "requirement", "path": "./datasets/eTour_en/UC"}
    },
    "target_artifact_provider": {
        "name": "text",
        "args": {"artifact_type": "source code", "path": "./datasets/eTour_en/CC"}
    },
    "source_preprocessor": {
        "name": "sentence",
        "args": {"language": "en"}
    },
    "target_preprocessor": {
        "name": "code_chunking",
        "args": {"language": "java"}
    },
    "embedding_creator": {
        "name": "open_ai",
        "args": {"path": "./storage/eTour_en/embeddings/", "model": "text-embedding-3-large"}
    },
    "source_store": {
        "name": "chroma",
        "args": {"direction": "source", "path": "./storage/eTour_en/"}
    },
    "target_store": {
        "name": "chroma",
        "args": {"direction": "target", "path": "./storage/eTour_en/", "similarity_function": "cosine", "n_results": 20}
    },
    "classifier": {
        "name": "chain_of_thought",
        "args": {"model": "gpt-3.5-turbo-0125"}
    },
    "result_aggregator": {
        "name": "any_connection",
        "args": {
            "source_granularity": 0
        }
    }
}